<div class="container text-center">
  <div class="row justify-content-md-center">
    <h2> Search Employee </h2> 
    <%= link_to "Back", employee_forms_path %>
    <div class="col">
      <br>
      <%= form_with url: "/search_employee", method: :get do |form| %>
        <%= form.label :employee_name, "Search for:" %>
        <%= form.text_field :employee_name %>
        <%= form.submit "Search" %>
      <% end %> <br/>
      <%= render "shared/search_table" %>      
    </div>
  </div>
</div>